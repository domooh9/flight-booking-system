  }\n      default: {\n        return response;\n      }\n    }\n  };\n\nexport const clearFlights = (flightData) => async (dispatch) => {\n  dispatch({ type: actionTypes.CLEAR_FILTER_FLIGHTS, payload: flightData });\n};\n","/home/dom/Videos/FlyM/src/components/AdminLogin.js",["115","116"],[],"import React, { useState, useEffect } from 'react';\nimport { Link, useNavigate } from 'react-router-dom';\n\nconst AdminLogin = () => {\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n  const [isAdminLoggedIn, setIsAdminLoggedIn] = useState(false);\n  const history = useNavigate();\n\n  useEffect(() => {\n    // Check if the user is already logged in\n    const isLoggedIn = localStorage.getItem('isAdminLoggedIn') === 'true';\n    setIsAdminLoggedIn(isLoggedIn);\n  }, []);\n\n  const handleUsernameChange = (event) => {\n    setUsername(event.target.value);\n  };\n\n  const handlePasswordChange = (event) => {\n    setPassword(event.target.value);\n  };\n\n  const handleLogin = (event) => {\n    event.preventDefault();\n    if (username === 'John Doe' && password === '12345') {\n      setIsAdminLoggedIn(true);\n      // Set the login status in local storage\n      localStorage.setItem('isAdminLoggedIn', true);\n      // Navigate to the Admindash component\n      history('/dash');\n    } else {\n      alert('Invalid username or password');\n    }\n  };\n\n \n\n  return (\n    <div className=\"Auth-form-container\">\n      \n        \n        <form className=\"Auth-form\" onSubmit={handleLogin}>\n          <div className=\"Auth-form-content\">\n            <h3 className=\"Auth-form-title\">Admin</h3>\n            <div className=\"form-group mt-3\">\n              <label>Username</label>\n              <input className=\"form-control mt-1\" type=\"text\" value={username} onChange={handleUsernameChange} />\n            </div>\n            <div className=\"form-group mt-3\">\n              <label>Password</label>\n              <inpu